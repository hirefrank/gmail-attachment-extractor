{
  "kvNamespace": "gmail-extractor-production",
  "cronSchedule": "0 0 * * 0",
  "logLevel": "info",
  "maxEmailsPerRun": 50,
  "maxAttachmentSize": 26214400,
  "environment": "production",
  "monitoring": {
    "healthCheckInterval": "5m",
    "alertThreshold": 5,
    "retentionDays": 30
  },
  "features": {
    "enableDetailedLogging": false,
    "enablePerformanceMetrics": true,
    "enableTestData": false
  }
}